callGraph(edges, nodes) ::=<<
digraph G {
        ranksep=.25;
        edge [arrowsize.5]
        node [shape=circle, fontname="ArialNarrow"],
        fontsize=12, fixedsize=true, height=.45
        <edges :{edge | <edge>;}>
        <nodes.keys:{str1 | <nodes.(str1) :{str2 | <str1> -> <str2>; }; separator="\n">}; separator="\n">
}
>>